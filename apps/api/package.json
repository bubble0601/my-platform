{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "prebuild": "rimraf dist",
    "build": "node ./build.js",
    "start": "node ./dist/index.js",
    "generate": "run-s -l generate:prisma generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "prisma:case": "npx prisma-case-format --file ./prisma/schema.prisma",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/etag": "^4.1.0",
    "@fastify/session": "^10.0.2",
    "@prisma/client": "^4.5.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "envsafe": "^2.0.3",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.3.0",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.5",
    "ioredis": "^5.2.4",
    "mercurius": "^11.3.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "superjson": "^1.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@internal/eslint-config": "*",
    "@internal/tsconfig": "*",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^16.0.0",
    "esbuild": "^0.15.15",
    "eslint": "^8.0.1",
    "prisma": "^4.5.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  }
}
