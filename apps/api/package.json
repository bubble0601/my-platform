{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "prebuild": "rimraf dist",
    "build": "node ./build.js",
    "start": "node ./dist/index.js",
    "fmt": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src --ext .ts --cache",
    "typechk": "tsc --noEmit",
    "generate": "run-s -l generate:prisma generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "prisma:case": "pnpm dlx prisma-case-format --file ./prisma/schema.prisma"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/session": "^10.1.1",
    "@prisma/client": "^4.10.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "envsafe": "^2.0.3",
    "fastify": "^4.14.0",
    "fastify-plugin": "^4.5.0",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.5",
    "ioredis": "^5.3.1",
    "mercurius": "^12.1.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.4",
    "superjson": "^1.12.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@internal/tsconfig": "workspace:*",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.14.0",
    "esbuild": "^0.17.8",
    "eslint": "^8.34.0",
    "pino-pretty": "^9.3.0",
    "prisma": "^4.10.1",
    "rimraf": "^4.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
