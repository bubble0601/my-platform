{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c auto -n app,db,gql 'pnpm:dev:app' 'pnpm:gen:prisma --watch' 'pnpm:gen:gql --watch'",
    "dev:app": "dotenv -- ts-node-dev --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "build": "rimraf dist && mkdir -p ./dist/schema && node ./build.js && cp ./src/schema/schema.gql ./dist/schema/schema.gql",
    "start": "dotenv -- node ./dist/index.js",
    "lint": "eslint src --cache",
    "lint:ci": "eslint src",
    "typechk": "tsc --noEmit",
    "gen": "pnpm run '/^gen:.*/'",
    "gen:prisma": "prisma generate",
    "gen:gql": "graphql-codegen",
    "db": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:dev": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "docker:up": "docker-compose -f ./test/docker-compose.yml up -d ",
    "docker:down": "docker-compose -f ./test/docker-compose.yml down",
    "test": "pnpm docker:up && dotenv -e .env.test pnpm prisma db push && dotenv -e .env.test vitest"
  },
  "dependencies": {
    "@envelop/generic-auth": "^6.1.1",
    "@prisma/client": "4.16.2",
    "@types/jsonwebtoken": "^9.0.4",
    "envsafe": "^2.0.3",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^4.0.5",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.26.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/graphql-modules-preset": "^4.0.1",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@internal/eslint-config": "workspace:*",
    "@internal/tsconfig": "workspace:^",
    "@types/internal": "workspace:^",
    "@types/node": "^20.8.7",
    "@types/pg": "^8.10.7",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "prisma": "^4.16.2",
    "prisma-kysely": "^1.7.1",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.5",
    "vitest": "^0.34.6"
  }
}
